{
      "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": "./src",
        "exclude": ["./src/webxr"],
        "includePattern": ".js$"
    },
    "plugins": [
        "plugins/markdown",
        "./jsdoc-preprocessor"
    ],
    "opts": {
        "template": "./jsdoc-template/",
        "encoding": "utf8",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}